{
  "paths":{
    "inputs": {
      "dir": "../data/input_data",
      "files": {
        "adjacency_matrix": "connectivity.npz",
        "neuron_info": "neuron_info.pickle",
        "raw_spikes": "raw_spikes.npy",
        "stimuli": "stim_stream.npy"
      }
    },
    "analyzed": {
      "dir": "../data/analyzed_data",
      "files": {
        "split_spikes": "split_spike_trains.npy",
        "database": "tribe_database.pkl",
        "tribes": "tribes.json",
        "struc_parameters": "structural_parameters.json",
        "components": "extracted_components.json",
        "ec_values": "ec_values.json",
        "features": "features.npy",
        "classifier_test_scores": "classifier_test_scores.npy",
        "classifier_cv_scores": "classifier_cv_scores.npy",
        "classifier_manifold_results": "classifier_manifold_results.json",
        "classification_hc": "classification_hc.h5",
        "accuracy_hc": "accuracy_hc.json",
        "accuracy": "accuracy.txt"
      }
    },
    "other": {
      "dir": "../data/other"
    },
    "config": {
      "dir": ".",
      "files": {
        "split_spikes": "input_data_config.json",
        "classifier": "classifier_config.json",
        "ec_analysis": "ec_analysis_config.json",
        "evaluator": "evaluator_config.json",
        "gen_topo_db": "topo_db_config.json",
        "manifold_analysis": "manifold_config.json",
        "sample_tribes": "sampling_config.json",
        "struc_tribe_analysis": "structural_analysis_config.json"
      }
    }
  },
  "structure": {
      "split_spikes": {
          "inputs": {
              "raw_spikes": "inputs",
              "stimuli": "inputs"
          },
          "outputs": {
              "split_spikes": "analyzed"
          }
      },
      "gen_topo_db": {
          "inputs": {
              "adjacency_matrix": "inputs",
              "neuron_info": "inputs"
          },
          "outputs": {
              "database": "analyzed"
          }
      },
      "sample_tribes": {
          "inputs": {
              "database": "analyzed"
          },
          "outputs": {
              "tribes": "analyzed"
          }
      },
      "struc_tribe_analysis": {
          "inputs": {
              "database": "analyzed",
              "tribes": "analyzed"
          },
          "outputs": {
              "struc_parameters": "analyzed"
          }
      },
      "manifold_analysis": {
          "inputs": {
             "raw_spikes": "inputs",
             "stimuli": "inputs",
             "tribes": "analyzed"
          },
          "outputs": {
             "components": "analyzed"
          }
      },
      "classifier": {
          "inputs": {
             "features": "analyzed",
             "components": "analyzed"
          },
          "outputs": {
             "classifier_test_scores": "analyzed",
             "classifier_cv_scores": "analyzed",
             "classifier_manifold_results": "analyzed"
          }
      },
      "evaluator": {
          "inputs": {
             "classifier_test_scores": "analyzed",
             "classifier_cv_scores": "analyzed"
          },
          "outputs": {
             "accuracy": "analyzed"
          }
      }
  }
}
